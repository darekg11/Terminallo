{
  "author": "Dariusz GÃ³rak <darekg11@tlen.pl>",
  "description": "Terminallo - app to handle multiple console windows in single app",
  "name": "terminallo",
  "version": "2.0.0",
  "main": "./src/index.js",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/plugin-proposal-function-bind": "7.2.0",
    "@babel/plugin-transform-async-to-generator": "7.2.0",
    "@babel/polyfill": "7.2.5",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-react": "7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "cross-env": "5.2.0",
    "css-loader": "2.1.0",
    "electron": "4.0.1",
    "electron-builder": "20.38.5",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "eslint": "5.12.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "3.0.1",
    "jest": "23.6.0",
    "prettier-eslint": "8.8.2",
    "redux-mock-store": "1.5.3",
    "regenerator-runtime": "0.13.1",
    "sinon": "7.2.3",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.1",
    "url-loader": "1.1.2",
    "webpack-cli": "3.2.1",
    "webpack-dev-server": "3.1.14"
  },
  "dependencies": {
    "@material-ui/core": "3.9.0",
    "@material-ui/icons": "3.0.2",
    "chokidar": "2.0.4",
    "electron-is-dev": "1.0.1",
    "fs-extra": "7.0.1",
    "jss": "9.8.7",
    "lodash": "4.17.11",
    "mousetrap": "1.6.2",
    "node-pty": "0.8.0",
    "prop-types": "15.6.2",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-redux": "5.1.1",
    "redux": "4.0.1",
    "redux-batched-actions": "0.4.1",
    "redux-thunk": "2.3.0",
    "typeface-roboto": "0.0.54",
    "uuid": "3.3.2",
    "webpack": "4.28.4",
    "xterm": "3.10.1"
  },
  "engines": {
    "node": "8.5.0"
  },
  "scripts": {
    "watch": "./node_modules/.bin/webpack-dev-server --config=webpack.dev.config.js",
    "start": "cross-env NODE_ENV=development electron .",
    "test": "jest",
    "test-watch": "npm test -- --watch",
    "build-dev": "./node_modules/.bin/webpack --config=webpack.dev.config.js && electron-builder",
    "build-prod": "./node_modules/.bin/webpack --config=webpack.prod.config.js && electron-builder"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "!src/index.js",
      "**/*.{js,jsx}",
      "!**/*.config.js",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/dist/**",
      "!**/public/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "html"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/src/tests/__mocks__/CSSMocksForJest.js"
    },
    "setupFiles": [
      "<rootDir>/jest.init.js"
    ]
  },
  "build": {
    "appId": "com.rapidcode.terminallo",
    "productName": "Terminallo",
    "files": [
      "**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}",
      "!.babelrc",
      "!.git/**/*",
      "!.eslintrc",
      "!.gitignore",
      "!babel.config.js",
      "!jest.init.js",
      "!LICENSE",
      "!npm_install.sh",
      "!package-lock.json",
      "!package.json",
      "!README.md",
      "!webpack.dev.config.js",
      "!webpack.prod.config.js",
      "!src/tests/**/*",
      "!src/app/**/*",
      "!coverage/**/*"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "artifactName": "${productName}-${version}-setup.${ext}"
    },
    "linux": {
      "category": "Utility",
      "target": [
        {
          "target": "deb",
          "arch": [
            "x64"
          ]
        }
      ],
      "artifactName": "${productName}-${version}-setup.${ext}"
    }
  }
}
