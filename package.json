{
  "author": "Dariusz GÃ³rak",
  "description": "Terminallo - app to handle multiple console windows in single app",
  "name": "terminallo",
  "version": "0.0.1",
  "main": "./src/index.js",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "electron": "^1.7.10",
    "electron-builder": "^19.54.0",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "file-loader": "^1.1.6",
    "prettier-eslint": "^8.8.1",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "electron-is-dev": "^0.3.0",
    "fs-extra": "^5.0.0",
    "lodash": "^4.17.4",
    "material-ui": "^1.0.0-beta.29",
    "material-ui-icons": "^1.0.0-beta.17",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "typeface-roboto": "0.0.50",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0"
  },
  "engines": {
    "node": "8.5.0"
  },
  "scripts": {
    "watch": "./node_modules/.bin/webpack-dev-server --config=webpack.dev.config.js",
    "start": "cross-env NODE_ENV=development ./node_modules/.bin/electron .",
    "build-dev": "./node_modules/.bin/webpack --config=webpack.dev.config.js && electron-builder",
    "build-prod": "./node_modules/.bin/webpack --config=webpack.prod.config.js && electron-builder"
  },
  "build": {
    "appId": "Terminallo",
    "productName": "Terminallo",
    "files": ["**/*", "!src/app/**/*"],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": ["x64", "ia32"]
        }
      ],
      "artifactName": "${productName}-${version}-setup.${ext}"
    }
  }
}
